package tester;

import com.app.tree.BinarySearchTree;
public class TestTree {

	public static void main(String[] args) {
		BinarySearchTree bst = new BinarySearchTree();
		
		bst.add(50);
		bst.add(30);
		bst.add(10);
		bst.add(20);
		bst.add(40);
		bst.add(90);
		bst.add(70);
		bst.add(60);
		bst.add(80);
		bst.add(100);
		bst.preorder();
		bst.inorder();
		bst.postorder();

	}

}
